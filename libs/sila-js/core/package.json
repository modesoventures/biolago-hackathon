  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b && yarn build:copy",
    "build:dev": "tsc -b -w",
    "build:docs": "npx typedoc",
    "build:proto": "node ./proto-gen-types",
    "build:copy": "mkdir -p dist/framework && cp -r src/framework/protobuf dist/framework && cp -r src/framework/feature_definitions dist/framework",
    "clean": "del-cli dist tsconfig.tsbuildinfo",
    "coverage": "jest --coverage",
    "prepare": "yarn build",
    "test": "jest",
    "test:dev": "jest --watch",
    "start": "yarn build && node dist/index.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.6",
    "@grpc/proto-loader": "^0.6.4",
    "protobufjs": "^6.11.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/uuid": "^8.3.1",
    "del-cli": "^3.0.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "private": true,
  "publishConfig": {
    "access": "public"
  }
