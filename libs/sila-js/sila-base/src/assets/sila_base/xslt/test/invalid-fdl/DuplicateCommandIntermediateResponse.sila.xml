<?xml version="1.0" encoding="utf-8" ?>
<Feature SiLA2Version="1.0" FeatureVersion="1.0" Originator="org.silastandard"
         Category="tests"
         xmlns="http://www.sila-standard.org"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.sila-standard.org https://gitlab.com/SiLA2/sila_base/raw/master/schema/FeatureDefinition.xsd">
    <Identifier>DuplicateCommandIntermediateResponse</Identifier>
    <DisplayName>Duplicate Command Intermediate Response</DisplayName>
    <Description>Contains a command with multiple intermediate responses with the same identifier</Description>
    <Command>
        <Identifier>TestCommand</Identifier>
        <DisplayName>Test Command</DisplayName>
        <Description>A test command</Description>
        <Observable>No</Observable>
        <IntermediateResponse>
            <Identifier>IntermediateResponse1</Identifier>
            <DisplayName>Intermediate Response 1</DisplayName>
            <Description>An intermediate response</Description>
            <DataType>
                <Basic>Integer</Basic>
            </DataType>
        </IntermediateResponse>
        <IntermediateResponse>
            <Identifier>IntermediateResponse1</Identifier>
            <DisplayName>Intermediate Response 1</DisplayName>
            <Description>Another intermediate response</Description>
            <DataType>
                <Basic>Integer</Basic>
            </DataType>
        </IntermediateResponse>
    </Command>
</Feature>