<?xml version="1.0" encoding="utf-8" ?>
<Feature SiLA2Version="1.0" FeatureVersion="1.0" Originator="org.silastandard"
         Category="tests"
         xmlns="http://www.sila-standard.org"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.sila-standard.org https://gitlab.com/SiLA2/sila_base/raw/master/schema/FeatureDefinition.xsd">
    <Identifier>DateSet</Identifier>
    <DisplayName>Date Set</DisplayName>
    <Description>Contains a Date with Set constraint</Description>
    <DataTypeDefinition>
        <Identifier>TestType</Identifier>
        <DisplayName>Test Type</DisplayName>
        <Description>A test type</Description>
        <DataType>
            <Constrained>
                <DataType>
                    <Basic>Date</Basic>
                </DataType>
                <Constraints>
                    <Set>
                        <Value>2020-01-01Z</Value>
                        <Value>2020-02-29-00:00</Value>
                        <Value>1990-01-13-14:00</Value>
                        <Value>1990-12-31+14:00</Value>
                        <Value>1990-10-05+02:30</Value>
                        <Value>2004-02-29+02:30</Value>
                        <Value>2000-02-29+02:30</Value>
                        <Value>0000-01-01+02:30</Value>
                        <Value>0000-01-01-14:00</Value>
                        <Value>9999-12-31+14:00</Value>
                    </Set>
                </Constraints>
            </Constrained>
        </DataType>
    </DataTypeDefinition>
</Feature>
