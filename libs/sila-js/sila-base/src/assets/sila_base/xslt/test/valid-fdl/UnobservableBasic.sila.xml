<?xml version="1.0" encoding="utf-8" ?>
<Feature SiLA2Version="1.0" FeatureVersion="1.0" Originator="org.silastandard"
         Category="tests"
         xmlns="http://www.sila-standard.org"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.sila-standard.org https://gitlab.com/SiLA2/sila_base/raw/master/schema/FeatureDefinition.xsd">
    <Identifier>UnobservableBasic</Identifier>
    <DisplayName>Unobservable Basic</DisplayName>
    <Description>Provides an unobservable command with basic return data types and basic parameters and an unobservable property with basic response type</Description>
    <Command>
        <Identifier>TestCommand</Identifier>
        <DisplayName>Test Command</DisplayName>
        <Description>Command for testing</Description>
        <Observable>No</Observable>
        <Parameter>
            <Identifier>Param1</Identifier>
            <DisplayName>Param 1</DisplayName>
            <Description>First parameter</Description>
            <DataType>
                <Basic>Integer</Basic>
            </DataType>
        </Parameter>
        <Parameter>
            <Identifier>Param2</Identifier>
            <DisplayName>Param 2</DisplayName>
            <Description>Second parameter</Description>
            <DataType>
                <Basic>String</Basic>
            </DataType>
        </Parameter>
        <Response>
            <Identifier>Response1</Identifier>
            <DisplayName>Response 1</DisplayName>
            <Description>First response</Description>
            <DataType>
                <Basic>String</Basic>
            </DataType>
        </Response>
        <Response>
            <Identifier>Response2</Identifier>
            <DisplayName>Response 2</DisplayName>
            <Description>Seconds response</Description>
            <DataType>
                <Basic>Boolean</Basic>
            </DataType>
        </Response>
    </Command>
    <Property>
        <Identifier>TestProperty</Identifier>
        <DisplayName>Test Property</DisplayName>
        <Description>Property for testing</Description>
        <Observable>No</Observable>
        <DataType>
            <Basic>Integer</Basic>
        </DataType>
    </Property>
</Feature>