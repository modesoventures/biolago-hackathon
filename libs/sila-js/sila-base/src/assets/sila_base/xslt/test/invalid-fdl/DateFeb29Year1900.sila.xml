<?xml version="1.0" encoding="utf-8" ?>
<Feature SiLA2Version="1.0" FeatureVersion="1.0" Originator="org.silastandard"
         Category="tests"
         xmlns="http://www.sila-standard.org"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.sila-standard.org https://gitlab.com/SiLA2/sila_base/raw/master/schema/FeatureDefinition.xsd">
    <Identifier>DateFeb29Year1900</Identifier>
    <DisplayName>Date Feb 29 Year 1900</DisplayName>
    <Description>Contains a Date with Set constraint with 1900-02-29</Description>
    <DataTypeDefinition>
        <Identifier>TestType</Identifier>
        <DisplayName>Test Type</DisplayName>
        <Description>A test type</Description>
        <DataType>
            <Constrained>
                <DataType>
                    <Basic>Date</Basic>
                </DataType>
                <Constraints>
                    <Set>
                        <Value>1900-02-29Z</Value>
                    </Set>
                </Constraints>
            </Constrained>
        </DataType>
    </DataTypeDefinition>
</Feature>
