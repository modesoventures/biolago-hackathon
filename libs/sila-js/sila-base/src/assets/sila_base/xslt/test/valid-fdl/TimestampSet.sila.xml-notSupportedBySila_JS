<?xml version="1.0" encoding="utf-8" ?>
<Feature SiLA2Version="1.0" FeatureVersion="1.0" Originator="org.silastandard"
         Category="tests"
         xmlns="http://www.sila-standard.org"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.sila-standard.org https://gitlab.com/SiLA2/sila_base/raw/master/schema/FeatureDefinition.xsd">
    <Identifier>TimestampSet</Identifier>
    <DisplayName>Timestamp Set</DisplayName>
    <Description>Contains a Timestamp with Set constraint</Description>
    <DataTypeDefinition>
        <Identifier>TestType</Identifier>
        <DisplayName>Test Type</DisplayName>
        <Description>A test type</Description>
        <DataType>
            <Constrained>
                <DataType>
                    <Basic>Timestamp</Basic>
                </DataType>
                <Constraints>
                    <Set>
                        <Value>2021-01-01T01:52:09Z</Value>
                        <Value>2021-12-31T15:30:27-00:00</Value>
                        <Value>2021-02-28T00:00:00+02:30</Value>
                    </Set>
                </Constraints>
            </Constrained>
        </DataType>
    </DataTypeDefinition>
</Feature>
